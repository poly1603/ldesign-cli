{
  "name": "@ldesign/cli-workspace",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "LDesign CLI 工具 - Monorepo 工作空间",
  "scripts": {
    "build": "pnpm -r --filter='./packages/*' build",
    "build:shared": "pnpm --filter @ldesign/cli-shared build",
    "build:server": "pnpm --filter @ldesign/cli-server build",
    "build:web-ui": "pnpm --filter @ldesign/cli-web-ui build",
    "build:cli": "pnpm --filter @ldesign/cli build",
    "dev": "pnpm --filter @ldesign/cli dev",
    "dev:server": "pnpm --filter @ldesign/cli-server dev",
    "dev:web": "pnpm --filter @ldesign/cli-web-ui dev",
    "clean": "pnpm -r --parallel exec rimraf dist",
    "test": "pnpm -r --filter='./packages/*' test",
    "lint": "pnpm -r --filter='./packages/*' lint"
  },
  "keywords": [
    "ldesign",
    "cli",
    "scaffold",
    "ui",
    "vue",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.3",
    "vitest": "^1.0.0"
  }
}
