{
  "name": "@ldesign/cli",
  "version": "1.0.0",
  "description": "LDesign CLI 工具 - 提供脚手架、UI 管理界面等功能",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ldesign": "./bin/cli.js",
    "ld": "./bin/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:quick": "tsx src/cli/index.ts ui --debug",
    "dev:web": "cd src/web && pnpm run dev",
    "dev:server": "tsx src/cli/index.ts ui --debug --no-open",
    "prebuild": "npm run clean",
    "build": "npm run build:web && npm run build:cli && npm run copy:web",
    "postbuild": "node scripts/verify-build.js",
    "build:cli": "tsup",
    "build:web": "cd src/web && pnpm run build",
    "build:full": "npm run build",
    "copy:web": "node scripts/copy-web-dist.js",
    "clean": "rimraf dist",
    "clean:all": "rimraf dist && cd src/web && rimraf dist node_modules",
    "test": "echo \"Tests coming soon\"",
    "lint": "echo \"Linting coming soon\"",
    "type-check": "tsc --noEmit",
    "start": "node dist/cli/index.js ui",
    "start:prod": "set NODE_ENV=production && node dist/cli/index.js ui",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ldesign",
    "cli",
    "scaffold",
    "ui",
    "vue",
    "typescript"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@ldesign/analyzer": "workspace:*",
    "@ldesign/builder": "workspace:*",
    "@ldesign/deployer": "workspace:*",
    "@ldesign/deps": "workspace:*",
    "@ldesign/docs-generator": "workspace:*",
    "@ldesign/git": "workspace:*",
    "@ldesign/generator": "workspace:*",
    "@ldesign/launcher": "workspace:*",
    "@ldesign/monitor": "workspace:*",
    "@ldesign/security": "workspace:*",
    "@ldesign/tester": "workspace:*",
    "better-sqlite3": "^11.7.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "open": "^10.0.3",
    "portfinder": "^1.0.32",
    "verdaccio": "^6.2.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@vitest/ui": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "sql.js": "^1.10.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "tsup": "^8.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "vitest": "^1.0.0",
    "@antfu/eslint-config": "^6.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
